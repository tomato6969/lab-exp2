#include <iostream>
#include <string>
using namespace std;

int main() {
    
    int number;
    cout<<"enter any number ";
    cin>>number;
    if (number > 0)
    {
        cout<<"the else is positive";
    }
    else if(number <0)
    {
        cout<<"the number is negative";
    }
    else
    {
        cout<<" the number is zero";
        
    }
    
    
    
    

   return 0;
}
OUTPUT:
enter any number 4
the number is positive
